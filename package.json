{
  "name": "@haiilo/n8n-nodes-haiilo",
  "version": "1.0.0",
  "description": "Haiilo Home nodes for n8n",
  "license": "MIT",
  "homepage": "",
  "keywords": [
    "n8n-community-node-package"
  ],
  "author": {
    "name": "Haiilo GmbH",
    "email": "developer@haiilo.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haiilo/n8n-node.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
