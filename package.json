  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/node": "^24.9.1",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-core": "^1.117.0"
